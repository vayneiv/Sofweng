<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.jerem.eatsmart.filter">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView11"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:src="@drawable/name_logo_transparentbg" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/scrollView4"
        android:layout_marginBottom="30dp"
        android:layout_below="@+id/imageView11">
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="@string/title_activity_filter_options"
            android:id="@+id/textView33"
            android:layout_centerHorizontal="true"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/text_title_cuisine"
            android:id="@+id/textView10"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/textView33"
            android:textColor="#000000"
            />

        <GridLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:columnCount="3"
            android:rowCount="3"
            android:id="@+id/gridLayout3"
            android:layout_gravity="center_horizontal"
            android:layout_below="@+id/textView10"
            android:layout_centerHorizontal="true">

            <CheckBox
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/text_cuisine_1"
                android:id="@+id/checkBox"
                android:layout_row="0"
                android:layout_column="0"
                android:enabled="true"
                android:clickable="true"
                android:textColor="#000000" />

            <CheckBox
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/text_cuisine_2"
                android:id="@+id/checkBox2"
                android:layout_row="0"
                android:layout_column="1"
                android:enabled="true"
                android:clickable="true"
                android:textColor="#000000" />
            <CheckBox
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/text_cuisine_3"
                android:id="@+id/checkBox8"
                android:layout_row="0"
                android:layout_column="2"
                android:clickable="true"
                android:enabled="true"
                android:textColor="#000000" />

            <CheckBox
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/text_cuisine_4"
                android:id="@+id/checkBox3"
                android:layout_row="1"
                android:layout_column="0"
                android:enabled="true"
                android:clickable="true"
                android:textColor="#000000" />

            <CheckBox
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/text_cuisine_5"
                android:id="@+id/checkBox4"
                android:layout_row="1"
                android:layout_column="1"
                android:textColor="#000000"
                android:enabled="true"
                android:clickable="true" />

            <CheckBox
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/text_cuisine_6"
                android:id="@+id/checkBox5"
                android:layout_row="1"
                android:layout_column="2"
                android:enabled="true"
                android:clickable="true"
                android:textColor="#000000" />

            <CheckBox
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/text_cuisine_7"
                android:id="@+id/checkBox6"
                android:layout_row="2"
                android:layout_column="0"
                android:enabled="true"
                android:clickable="true"
                android:textColor="#000000" />

            <CheckBox
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/text_cuisine_8"
                android:id="@+id/checkBox7"
                android:layout_row="2"
                android:layout_column="1"
                android:enabled="true"
                android:clickable="true"
                android:textColor="#000000" />



            <CheckBox
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/text_cuisine_9"
                android:id="@+id/checkBox9"
                android:layout_row="2"
                android:layout_column="2"
                android:allowUndo="true"
                android:enabled="true"
                android:clickable="true"
                android:textColor="#000000" />
        </GridLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Budget"
            android:id="@+id/textView36"
            android:layout_gravity="center_horizontal"
            android:textColor="#000000"
            android:layout_below="@+id/gridLayout3"
            android:layout_centerHorizontal="true" />

        <RadioGroup
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/radioGroup5"
            android:orientation="horizontal"
            android:layout_below="@+id/textView36"
            android:layout_centerHorizontal="true">
            <RadioButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/text_budget_1"
                android:id="@+id/radioButton39"
                />
            <RadioButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/text_budget_2"
                android:id="@+id/radioButton40"

                />
            <RadioButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/text_budget_3"
                android:id="@+id/radioButton41"
                />
            <RadioButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/text_budget_4"
                android:id="@+id/radioButton42"
                />
        </RadioGroup>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Location"
            android:id="@+id/textView37"
            android:layout_below="@+id/radioGroup5"
            android:textColor="#000000"
            android:layout_centerHorizontal="true"
             />

        <RadioGroup
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView37"
            android:layout_centerHorizontal="true"
            android:id="@+id/radioGroup6">
            <RadioButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/text_location_1"
                android:id="@+id/radioButton43"
                />

            <RadioButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/text_location_2"
                android:id="@+id/radioButton44"
                />

            <RadioButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/text_location_3"
                android:id="@+id/radioButton45"
                />

            <RadioButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/text_location_4"
                android:id="@+id/radioButton46"
                />

            <RadioButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/text_location_5"
                android:id="@+id/radioButton47"
                />

            <RadioButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/text_location_6"
                android:id="@+id/radioButton48"
                />
        </RadioGroup>
    </RelativeLayout>

    </ScrollView>


</RelativeLayout>
