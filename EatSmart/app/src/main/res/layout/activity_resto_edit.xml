<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.jerem.eatsmart.resto_edit"
    android:background="#da0101">

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton6"
        android:src="@drawable/signup_bt"
        android:background="#00ffffff"
        android:maxHeight="120dp"
        android:maxWidth="120dp"
        android:minHeight="120dp"
        android:minWidth="120dp"
        android:scaleType="fitXY"
        android:adjustViewBounds="true"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/button12"
        android:layout_alignEnd="@+id/button12" />

    <ImageView
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:id="@+id/imageView8"
        android:foreground="#00ffffff"
        android:scaleType="fitXY"
        android:background="@null"
        android:src="@drawable/profile_logo"
        android:nestedScrollingEnabled="false"
        android:longClickable="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:adjustViewBounds="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Select Image"
        android:id="@+id/button12"
        android:layout_below="@id/imageView8"
        android:layout_centerHorizontal="true" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/scrollView3"
        android:layout_above="@+id/imageButton6"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/button12">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Description"
                android:id="@+id/textView22"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_gravity="center_horizontal"
                android:textColor="#ffffff" />

            <EditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/editText3"
                android:layout_above="@+id/textView3"
                android:layout_centerHorizontal="true"
                android:layout_gravity="center_horizontal"
                android:background="#ffffff" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Contact No."
                android:id="@+id/textView26"
                android:layout_below="@+id/editText4"
                android:layout_centerHorizontal="true"
                android:textColor="#ffffff"
                android:layout_marginTop="0dp"
                android:layout_gravity="center_horizontal" />

            <EditText
                android:layout_width="369dp"
                android:layout_height="wrap_content"
                android:id="@+id/editText11"
                android:layout_below="@+id/textView7"
                android:layout_alignLeft="@+id/editText6"
                android:layout_alignStart="@+id/editText6"
                android:layout_alignRight="@+id/editText6"
                android:layout_alignEnd="@+id/editText6"
                android:background="#ffffff"
                android:inputType="phone"
                android:layout_gravity="center_horizontal" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Category"
                android:id="@+id/textView30"
                android:layout_alignParentBottom="true"
                android:layout_above="@+id/editText7"
                android:layout_alignRight="@+id/textView3"
                android:layout_alignEnd="@+id/textView3"
                android:layout_gravity="center_horizontal"
                android:textColor="#ffffff" />

            <GridLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:columnCount="3"
                android:rowCount="3"
                android:id="@+id/gridLayout"
                android:layout_above="@+id/editText16"
                android:layout_alignTop="@+id/textView22"
                android:layout_centerHorizontal="true"
                android:layout_gravity="center_horizontal" >

                <CheckBox
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Cafe"
                    android:id="@+id/checkBox14"
                    android:layout_row="0"
                    android:layout_column="0"
                    android:allowUndo="true"
                    android:enabled="true"
                    android:clickable="true"
                    android:textColor="#ffffff" />

                <CheckBox
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Dessert"
                    android:id="@+id/checkBox15"
                    android:layout_row="0"
                    android:layout_column="1"
                    android:allowUndo="true"
                    android:enabled="true"
                    android:clickable="true"
                    android:textColor="#ffffff" />

                <CheckBox
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/bar"
                    android:id="@+id/checkBox16"
                    android:layout_row="1"
                    android:layout_column="0"
                    android:allowUndo="true"
                    android:enabled="true"
                    android:clickable="true"
                    android:textColor="#ffffff" />

                <CheckBox
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Grill"
                    android:id="@+id/checkBox17"
                    android:layout_row="1"
                    android:layout_column="1"
                    android:textColor="#ffffff"
                    android:elegantTextHeight="false"
                    android:enabled="true"
                    android:allowUndo="true"
                    android:clickable="true" />

                <CheckBox
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Fine Dining"
                    android:id="@+id/checkBox18"
                    android:layout_row="1"
                    android:layout_column="2"
                    android:allowUndo="true"
                    android:enabled="true"
                    android:clickable="true"
                    android:textColor="#ffffff" />

                <CheckBox
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Veg"
                    android:id="@+id/checkBox19"
                    android:layout_row="2"
                    android:layout_column="0"
                    android:allowUndo="true"
                    android:enabled="true"
                    android:clickable="true"
                    android:textColor="#ffffff" />

                <CheckBox
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Buffet"
                    android:id="@+id/checkBox20"
                    android:layout_row="2"
                    android:layout_column="1"
                    android:allowUndo="true"
                    android:enabled="true"
                    android:clickable="true"
                    android:textColor="#ffffff" />

                <CheckBox
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Lutong Bahay"
                    android:id="@+id/checkBox21"
                    android:layout_row="0"
                    android:layout_column="2"
                    android:allowUndo="true"
                    android:clickable="true"
                    android:enabled="true"
                    android:textColor="#ffffff" />

                <CheckBox
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Fast Food"
                    android:id="@+id/checkBox22"
                    android:layout_row="2"
                    android:layout_column="2"
                    android:allowUndo="true"
                    android:enabled="true"
                    android:clickable="true"
                    android:textColor="#ffffff" />
            </GridLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Budget"
                android:id="@+id/textView31"
                android:layout_above="@+id/gridLayout"
                android:layout_alignTop="@+id/imageButton2"
                android:layout_alignLeft="@+id/imageButton2"
                android:layout_alignStart="@+id/imageButton2"
                android:layout_gravity="center_horizontal" />

            <RadioGroup
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/radioGroup9"
                android:orientation="horizontal"
                android:layout_below="@+id/textView20"
                android:layout_centerHorizontal="true">
                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/text_budget_1"
                    android:textColor="#ffffff"
                    android:id="@+id/radioButton40"
                    />
                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/text_budget_2"
                    android:textColor="#ffffff"
                    android:id="@+id/radioButton41"

                    />
                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/text_budget_3"
                    android:textColor="#ffffff"
                    android:id="@+id/radioButton42"
                    />
                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/text_budget_4"
                    android:textColor="#ffffff"
                    android:id="@+id/radioButton43"
                    />
            </RadioGroup>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Location"
                android:id="@+id/textView21"
                android:layout_centerVertical="true"
                android:layout_alignLeft="@+id/textView13"
                android:layout_alignStart="@+id/textView13"
                android:layout_gravity="center_horizontal"
                android:textColor="#ffffff" />

            <RadioGroup
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textView37"
                android:layout_centerHorizontal="true"
                android:gravity="center_vertical"
                android:id="@+id/radioGroup10">
                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/text_location_1"
                    android:textColor="#ffffff"
                    android:id="@+id/radioButton44"
                    />

                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/text_location_2"
                    android:textColor="#ffffff"
                    android:id="@+id/radioButton45"
                    />

                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/text_location_3"
                    android:textColor="#ffffff"
                    android:id="@+id/radioButton46"
                    />

                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/text_location_4"
                    android:textColor="#ffffff"
                    android:id="@+id/radioButton47"
                    />

                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/text_location_5"
                    android:textColor="#ffffff"
                    android:id="@+id/radioButton48"
                    />

                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/text_location_6"
                    android:textColor="#ffffff"
                    android:id="@+id/radioButton49"
                    />
            </RadioGroup>
        </LinearLayout>
    </ScrollView>
</RelativeLayout>